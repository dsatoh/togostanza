{
  "name": "<%- name %>",
  "version": "0.0.1",
  "keywords": [
    "togostanza-provider"
  ],
  "license": "<%- license %>",
  <%_ if (!skipGit) { -%>
  "repository": "<%- owner %>/<%- repo %>",
  <%_ } -%>
  "scripts": {
    "start": "togostanza serve --port $npm_package_config_port",
    "build": "togostanza build",
    "new-stanza": "togostanza new-stanza"
  },
  "config": {
    "port": "8080"
  },
  "dependencies": {
    "togostanza": "togostanza/ts#js"
  },
  "engines": {
    "node": ">=12"
  },
  "private": true
}
