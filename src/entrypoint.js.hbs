import { defineStanzaElement } from 'togostanza/src/stanza-element.mjs';
import main from './{{metadata.[@id]}}/index.js';

const metadata = {{to-json metadata}};
const outer    = {{to-json outer}};

const templates = [
  {{#each templates as |template|}}
    [{{to-json template.name}}, {{template.spec}}],
  {{/each}}
];

defineStanzaElement(main, {metadata, templates, outer, url: import.meta.url});
